%!PS

% Debug with:
% gs -sDEVICE=nullpage -dNOPAUSE -c save pop -f fizzy.ps -c quit

/fizzbuzz {
  /n exch def
  /a n 3 mod def
  /b n 5 mod def
  a 0 eq b 0 eq and
  {
    (FizzBuzz)
  } {
    a 0 eq
    {
      (Fizz)
    }
    {
      b 0 eq
      {
        (Buzz)
      }
      {
        n =string cvs
      } ifelse
    } ifelse
  } ifelse
} def

/Courier
/fontsize 11 def
fontsize selectfont

/x 72 def
/y 740 def

x y moveto

1 1 100 {
  /i exch def

  x
  y i 61 mod 1 add fontsize mul sub
  moveto

  i fizzbuzz show

  i 60 eq { showpage } if

} for

showpage
