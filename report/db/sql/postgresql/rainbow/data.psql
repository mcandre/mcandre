--() { :; }; exec psql -f "$0"

INSERT INTO "rainbow" ("hash", "password")
    SELECT md5('password'), 'password'
    WHERE NOT EXISTS
        (SELECT 1 FROM "rainbow" WHERE "hash" = md5('password'));

INSERT INTO "rainbow" ("hash", "password")
    SELECT md5('admin'), 'admin'
    WHERE NOT EXISTS
        (SELECT 1 FROM "rainbow" WHERE "hash" = md5('admin'));
